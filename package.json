{
  "name": "shared-workshop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "turbo run check:types check:format check:lint",
    "check:format": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "check:format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "check:lint": "next lint",
    "check:types": "tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@codesandbox/sandpack-react": "2.20.0",
    "@monaco-editor/react": "4.7.0-rc.0",
    "@prisma/client": "^6.5.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "jotai": "2.12.5",
    "monaco-editor": "0.52.2",
    "next": "^15.2.3",
    "next-auth": "5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "turbo": "2.5.4",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "9.31.0",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "eslint-config-turbo": "2.5.5",
    "eslint-plugin-tailwindcss": "4.0.0-beta.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.12.2"
